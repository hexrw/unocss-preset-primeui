{
  "name": "unocss-plugin-primeui",
  "description": "A super-modern UnoCSS plugin for PrimeVue. Single-file, zero-dependency, with full CI/CD and auto-publishing.",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "bun run clean && bun tsc && bun run minify",
    "clean": "rm -rf dist",
    "minify": "bunx terser dist/index.js -o dist/index.js --compress --mangle",
    "lint": "bunx eslint src --ext .ts",
    "prepare": "bun run build"
  },
  "peerDependencies": {
    "unocss": ">=0.60.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/node": "^20.11.30",
    "eslint": "^8.56.0",
    "terser": "^5.30.3",
    "unocss": "^0.60.0"
  },
  "keywords": ["unocss", "primevue", "plugin", "preset", "css", "utility"],
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "MIT"
}
